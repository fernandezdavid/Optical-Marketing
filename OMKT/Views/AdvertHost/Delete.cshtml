@model OMKT.Business.AdvertHost

@{
    ViewBag.Title = "Puestos";
    Layout = "~/Views/Shared/_Dashboard_Layout.cshtml";
}

<h2>Eliminar</h2>

<div class="alert alert-info">Estás seguro que deseas borrar este puesto?</div>
<fieldset>
    <legend>Puestos</legend>

    <div class="display-label">AdvertHostName</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AdvertHostName)
    </div>

    <div class="display-label">AdvertHostCategory</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AdvertHostCategory.Name)
    </div>

    <div class="display-label">StartTime</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.StartTime)
    </div>

    <div class="display-label">EndTime</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.EndTime)
    </div>

    <div class="display-label">LastDownTime</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.LastDownTime)
    </div>

    <div class="display-label">UpTime</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.UpTime)
    </div>

    <div class="display-label">Location</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Location.Latitude)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
         <input type="submit" value="Borrar" class="btn btn-primary" />
    </p>
}
