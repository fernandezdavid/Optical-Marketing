@model OMKT.Business.Video

@{
    ViewBag.Title = "Crear";
    Layout = "~/Views/Shared/_Dashboard_Layout.cshtml";
}
<div class="span12">
    <h2>
        Nuevo Juego
    </h2>
    @{Html.RenderPartial("DisplayMessages");}
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    @using (Html.BeginForm("Create", "Video", FormMethod.Post, new { @enctype = "multipart/form-data", @class = "form-horizontal" }))
    {
        @Html.ValidationSummary(true)
        <text>
        @{Html.RenderPartial("EditOrCreateVideoPartial", Model);}
        </text>
    }
</div>