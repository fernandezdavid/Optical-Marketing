@model IEnumerable<OMKT.Business.CatalogDetail>

<table  class="zebra-striped">
    <thead>
        <th>
            
        </th>
        <th>
            Producto
        </th>
        <th>
            Orden 
        </th>
        <th>
            Descuento
        </th>
        <th>
            QR
        </th>
        <th>
            URL
        </th>
        <th>
            Fecha de agregación
        </th>
        <th>
            
        </th>
    </thead>

@foreach (var item in Model) {
    var catalogDetail = "catalogDetail" + item.CatalogDetailId; 
    <tr id="@catalogDetail">
        <td>
            <img src="http://hp.static.adidas.com/@Url.Content(item.CommercialProduct.ProductImage.ThumbnailPath)" class="img-rounded" title="@item.CommercialProduct.ProductImage.Title" alt="@item.CommercialProduct.ProductImage.Caption" width="50px" height="50px"/>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CommercialProduct.ProductName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Discount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QRCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Link )
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Position)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedDate)
        </td>
        <td nowrap>
            <div class="btn-group">
                <a href="@Url.Action("Edit", new { id = item.CatalogDetailId })" class="btn primary small editAdvertDetails" idCatalogDetails="@item.CatalogDetailId"><i class=" icon-edit icon-white"></i></a>
        @*        <a href="@Url.Action("Details", new { id = item.AdvertDetailId })" class="btn small"><i class=" icon-align-justify "></i></a>*@
                <a href="@Url.Action("Delete", new { id = item.CatalogDetailId })" idCatalogDetails = "@item.CatalogDetailId" class="btn small deleteAdvertDetails"><i class=" icon-trash "></i></a>
            </div>
        </td>
    </tr>
}
    
</table>
