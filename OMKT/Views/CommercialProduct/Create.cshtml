@model OMKT.Business.CommercialProduct
@{
    ViewBag.Title = "Nuevo Producto";
    Layout = "~/Views/Shared/_Dashboard_Layout.cshtml";
}
<div class="span12">
<h2>Nuevo Producto</h2>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@{Html.RenderPartial("DisplayMessages");}
@using (Html.BeginForm("Create", "CommercialProduct", FormMethod.Post, new { @enctype = "multipart/form-data", @class = "form-horizontal" }))
{
    @Html.ValidationSummary(true)
    <text>
    @{Html.RenderPartial("EditOrCreateCommercialProductPartial", Model);}
    </text> 
}
</div>